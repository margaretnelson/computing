# Question 1
colors = ["teal","yellow", "periwinkle"]
print(colors)

# Question 2
nums = list(range(30, 60, 3))
print(type(nums))
print(nums)

# Question 3
dictionary = {"Sunny": "play", "Rainy": "watching TV", "Cloudy": "walk"}
print(type(dictionary))
print(dictionary)

dictionary.update({"snowy": "ski"})
print(dictionary)

# Question 4
score = 82
if score >=90:
    print("A")
elif score >= 80 and < 90:
    print("B")
elif score >= 70 and < 80:
    print("C")
else score < 70:
    print("D")
